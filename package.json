{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.6.3",
  "scripts": {
    "build": "esno ./scripts/rss.ts && cross-env NODE_OPTIONS=--max-old-space-size=4096 && vite-ssg build",
    "dev": "vite --port 3333 --open --host",
    "lint": "eslint .",
    "fix-case": "npx case-police --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.0-rc.13",
    "@unocss/reset": "^66.1.0-beta.5",
    "@vueuse/core": "^13.0.0",
    "@vueuse/head": "^2.0.0",
    "beasties": "^0.2.0",
    "dayjs": "^1.11.13",
    "markdown-it-table-of-contents": "^0.9.0",
    "nprogress": "^0.2.0",
    "typed.js": "^2.1.0",
    "unplugin-vue-markdown": "^28.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/logos": "^1.2.4",
    "@iconify/utils": "^2.3.0",
    "@shikijs/markdown-it": "^3.2.1",
    "@shikijs/transformers": "^3.2.1",
    "@shikijs/twoslash": "^3.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^22.13.10",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^66.1.0-beta.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@waline/client": "^3.5.6",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "enso": "1.0.0-alpha.3",
    "eslint": "^9.22.0",
    "eslint-plugin-case-police": "^1.0.0",
    "eslint-plugin-format": "^1.0.1",
    "esno": "^4.8.0",
    "feed": "^4.2.2",
    "fs-extra": "^11.3.0",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.5.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-link-attributes": "^4.0.1",
    "pnpm": "^10.6.3",
    "shiki": "^3.2.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^19.0.2",
    "typescript": "^5.8.2",
    "unocss": "^66.1.0-beta.5",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.2",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-inspect": "^11.0.0",
    "vite-plugin-pages": "^0.32.5",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-webfont-dl": "^3.10.4",
    "vite-ssg": "^25.2.0",
    "vite-ssg-sitemap": "^0.8.1",
    "vue-router-better-scroller": "^0.0.0",
    "vue-tsc": "^2.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint --fix"
  }
}
